{
  "name": "backend-main",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --loader ts-node/esm dist/src/index.js",
    "dev": "npm run build && npm run start",
    "build:tsc": "tsoa spec-and-routes && tsc",
    "build:post": "copyfiles -u 3 src/infrastructure/openapi/redoc.html dist/src/infrastructure/openapi/ && node src/infrastructure/openapi/postProcessSwagger.js",
    "build": "npm run build:tsc && npm run build:post && ([ \"$NODE_ENV\" != \"dev\" ] && npm run sentry:sourcemaps || echo Skipping sentry:sourcemaps in dev environment)",
    "prisma:studio": "npx prisma db pull && npx prisma studio",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org hurtle --project hurtle-main-backend ./dist && sentry-cli sourcemaps upload --org hurtle --project hurtle-main-backend ./dist"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed/university.seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@sentry/cli": "^2.23.0",
    "@sentry/node": "^7.88.0",
    "@sentry/profiling-node": "^1.3.2",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bullmq": "^5.4.2",
    "camelcase": "^6.0.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^8.4.1",
    "fast_array_intersect": "^1.1.0",
    "google-auth-library": "^9.7.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime": "^4.0.1",
    "moment-timezone": "^0.5.43",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-telegram-bot-api": "^0.64.0",
    "nodemailer": "^6.9.7",
    "openai": "^4.33.0",
    "otp-generator": "^4.0.1",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "qs": "^6.11.2",
    "redoc": "^2.1.3",
    "socks-proxy-agent": "^8.0.2",
    "squirrelly": "^9.0.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "uuid": "^9.0.1",
    "yup": "^1.4.0",
    "yup-locales": "^1.2.20"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-ts": "^1.7.2",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/node-telegram-bot-api": "^0.64.1",
    "@types/nodemailer": "^6.4.14",
    "@types/otp-generator": "^4.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "tsdef": "^0.0.14",
    "tsyringe": "^4.8.0",
    "typescript": "^5.3.3"
  }
}
